<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="id" format="reference" />
    <!-- The title associated with the item. -->
    <attr name="title" format="string|reference" />
    <attr name="icon_enable" format="reference" />
    <attr name="icon_disable" format="reference" />
    <!-- Whether the item is enabled. -->
    <attr name="enabled" format="boolean" />

    <!-- Base attributes that are available to all Item objects. -->
    <declare-styleable name="ZyMenuItem">
        <!-- The ID of the item. -->
        <attr name="id" />
        <!-- The title associated with the item. -->
        <attr name="title" />
        <!-- the icon menu enable -->
        <attr name="icon_enable" />
        <!-- the icon menu disable -->
        <attr name="icon_disable" />
        <!-- Whether the item is enabled. -->
        <attr name="enabled" />
    </declare-styleable>

</resources>